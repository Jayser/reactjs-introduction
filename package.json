{
  "name": "reactjs-synoptik",
  "version": "1.0.0",
  "description": "simple app",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint": "eslint app --config=configs/eslint/.eslintrc",
    "watch": "webpack --colors --watch",
    "server": "better-npm-run server",
    "build:dist": "better-npm-run build:dist",
    "build:prod": "better-npm-run build:prod",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test"
  },
  "betterScripts": {
    "server": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "build:dist": {
      "command": "webpack --display-modules"
    },
    "build:prod": {
      "command": "webpack --config --progress --colors",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "test": {
      "command": "karma start --single-run --no-auto-watch karma.conf.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "karma start --auto-watch --no-single-run karma.conf.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jayser/reactjs-synoptik.git"
  },
  "author": "Sarhan Azizov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jayser/reactjs-synoptik/issues"
  },
  "homepage": "https://github.com/Jayser/reactjs-synoptik#readme",
  "dependencies": {
    "babel-preset-react": "^6.11.1",
    "font-awesome": "^4.6.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "skycons": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.0",
    "file-loader": "^0.9.0",
    "force-case-sensitivity-webpack-plugin": "^0.1.1",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.9.3",
    "open": "0.0.5",
    "pre-git": "^3.10.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.15.0",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7",
    "write-file-webpack-plugin": "^3.3.0"
  },
  "config": {
    "pre-git": {
      "msg-pattern": "^feature\\-\\d{1,4}\\:.+",
      "pre-commit": [
        "npm run lint"
      ]
    }
  }
}
